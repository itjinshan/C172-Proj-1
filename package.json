{
  "name": "Node-Website-EC2-Sample-App",
  "description": "Sample NodeJs web application",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "mocha": "5.0.0",
    "supertest": "3.0.0",
    "unit.js": "^2.1.0"
  },
  "scripts": {
    "test": "mocha --recursive tests --exit",
    "client-install": "npm install --prefix frontend",
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "body-parse": "^0.1.0",
    "bootstrap": "^4.3.1",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "react-bootstrap": "^1.0.0-beta.14"
  }
}
